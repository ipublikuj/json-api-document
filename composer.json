{
  "name" : "ipub/json-api-document",
  "type" : "library",
  "description" : "JSON:API document factory",
  "keywords" : [
    "jsonapi",
    "json",
    "api",
    "client",
    "document",
    "ipub",
    "ipublikuj",
    "tools"
  ],

  "homepage" : "https://github.com/iPublikuj/json-api-document",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "https://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@fastybird.com",
    "issues" : "https://github.com/iPublikuj/json-api-document/issues"
  },

  "prefer-stable" : true,

  "minimum-stability" : "dev",

  "require" : {
    "php" : ">=7.4.0",
    "ext-json" : "*"
  },

  "require-dev" : {
    "mockery/mockery": "^1.4",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "pds/skeleton": "~1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },

  "autoload" : {
    "psr-4" : {
      "IPub\\JsonAPIDocument\\" : "src/"
    }
  },

  "config" : {
    "sort-packages" : true
  }
}
